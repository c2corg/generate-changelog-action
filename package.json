{
  "name": "generate-changelog-action",
  "version": "1.3.5",
  "description": "A Github Action to generate CHANGELOG.md, adapted to c2c_ui repository needs",
  "private": true,
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --check \"**/*.{ts,js,json,css,scss,less,md,html}\" && eslint src/**",
    "pack": "ncc build",
    "all": "npm run graphql && npm run lint && npm run build && npm run pack",
    "prepare": "husky install",
    "graphql": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c2corg/generate-changelog-action.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "Camptocamp Association",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c2corg/generate-changelog-action/issues"
  },
  "homepage": "https://github.com/c2corg/generate-changelog-action#readme",
  "dependencies": {
    "@actions/core": "1.9.0",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.0.3",
    "@octokit/graphql": "4.8.0",
    "graphql": "16.5.0",
    "handlebars": "4.7.7",
    "semver": "7.3.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.7.0",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-document-nodes": "2.2.13",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@octokit/graphql-schema": "10.74.1",
    "@tsconfig/node16-strictest": "1.0.3",
    "@tsconfig/recommended": "1.0.1",
    "@types/node": "18.0.1",
    "@types/semver": "7.3.10",
    "@typescript-eslint/eslint-plugin": "5.30.4",
    "@typescript-eslint/parser": "5.30.4",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  }
}
